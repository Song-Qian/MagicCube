{
    "presets": ["@babel/preset-env"],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-object-rest-spread",
        ["@babel/plugin-proposal-decorators", { "legacy" : true }],
        [
            "@babel/plugin-transform-typescript",
            {
              "isTSX": true,
              "allowNamespace": true
            }
        ]
    ]
}